{
	"appId": "international.idems.plh_teens",
	"appName": "PLH Teens",
	"bundledWebRuntime": false,
	"webDir": "www",
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 7000,
			"launchAutoHide": true
		},
		"CapacitorFirebaseAuth": {
			"providers": [
				"google.com"
			],
			"languageCode": "en",
			"nativeAuth": false,
			"permissions": {
				"google": [
					"profile"
				]
			}
		}
	}
}
