          "_translations": {
            "value": {}
          }
        },
        {
          "type": "items",
          "name": "loop_deployment_partners",
          "value": "@local.partners_deployment_data",
          "rows": [
            {
              "type": "image",
              "name": "@item.id",
              "value": "@item.image_asset",
              "_translations": {
                "value": {}
              },
              "condition": "@item.splash_page == 2"
